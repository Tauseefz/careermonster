    <title>Resume Builder</title>
    <script src="~/jQuery/jQuery-2.1.4.min.js"></script>

    <script type='text/javascript'>
        function deactiveother() {
            //$('#academics').hide();
            $('#personal').show();
            $('#academics').hide();
            $('#project').hide();
            $('#expe').hide();
            $('#achive').hide();
            $('#skills').hide();
            $('#language').hide();
        }
        function deactiveother1() {
            $('#academics').show();
            $('#personal').hide();
            $('#project').hide();
            $('#expe').hide();
            $('#achive').hide();
            $('#skills').hide();
            $('#language').hide();
        }
        function deactiveother2() {
            $('#academics').hide();
            $('#personal').hide();
            $('#project').show();
            $('#expe').hide();
            $('#achive').hide();
            $('#skills').hide();
            $('#language').hide();
        }
        function deactiveother3() {
            $('#academics').hide();
            $('#personal').hide();
            $('#project').hide();
            $('#expe').show();
            $('#achive').hide();
            $('#skills').hide();
            $('#language').hide();
        }
        function deactiveother4() {
            $('#academics').hide();
            $('#personal').hide();
            $('#project').hide();
            $('#expe').hide();
            $('#achive').show();
            $('#skills').hide();
            $('#language').hide();
        }
        function deactiveother5() {
            $('#academics').hide();
            $('#personal').hide();
            $('#project').hide();
            $('#expe').hide();
            $('#achive').hide();
            $('#skills').show();
            $('#language').hide();
        }
        function deactiveother6() {
            $('#academics').hide();
            $('#personal').hide();
            $('#project').hide();
            $('#expe').hide();
            $('#achive').hide();
            $('#skills').hide();
            $('#language').show();
        }
        $(document).ready(function () {
            $('#academics').hide();
            $('#project').hide();
            $('#expe').hide();
            $('#achive').hide();
            $('#skills').hide();
            $('#language').hide();

            $('#person').click(deactiveother);
            $('#academic').click(deactiveother1);
            $('#projects').click(deactiveother2);
            $('#experience').click(deactiveother3);
            $('#achieve').click(deactiveother4);
            $('#skill').click(deactiveother5);
            $('#lang').click(deactiveother6);

        });

    </script>
